{
  "mcpServers": {
    "mcp-gopls": {
      "type": "stdio",
      "command": "mcp-gopls"
    },
    "context7": {
      "type": "stdio", 
      "command": "npx",
      "args": [
        "@context7/mcp-server"
      ]
    },
    "graphql": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "mcp-graphql"
      ],
      "env": {
        "ENDPOINT": "https://demo.app.spacelift.io/graphql"
      }
    }
  }
}
